<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
        <add key="SKB.NuGet" value="https://nuget.pkg.github.com/saapo-ka-baadshah/index.json" />
    </packageSources>
    <packageSourceCredentials>
        <SKB.NuGet>
            <add key="Username" value="%NUGET_OWNER_USERNAME%"/>
